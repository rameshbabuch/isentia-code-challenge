{
  "name": "node-api",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js",
    "test": "mocha --require babel-register tests/**/*.js --exit",
    "lint": "eslint src/**/*.js "
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^4.19.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1"
  }
}
